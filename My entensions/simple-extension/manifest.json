 {
    "name": "Support Ticket Helper",
    "version": "1.0",
    "description": "Collect your ticket URL & the subject of the Ticket and send it to your Google Excel Sheet.",
    "permissions": ["activeTab", "declarativeContent", "storage"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [{
      "js": [
        "js/store-url.js",
        "https://cdnjs.cloudflare.com/ajax/libs/jquery/3.4.1/jquery.min.js",
      ],
      "matches": ["https://dashboard.helpninja.com/*"]
    }],
    "page_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/s-icon-16.png",
        "32": "images/s-icon-32.png",
        "48": "images/s-icon-48.png",
        "128": "images/s-icon-128.png"
      }
    },
    "icons": {
      "16": "images/s-icon-16.png",
      "32": "images/s-icon-32.png",
      "48": "images/s-icon-48.png",
      "128": "images/s-icon-128.png"
    },
    "options_page": "options.html",
    "manifest_version": 2
  }